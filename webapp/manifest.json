{
  "_version": "1.65.0",
  "sap.app": {
    "id": "app.miningjsonproject",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.3",
      "toolsId": "0901445d-208a-48fc-90e9-8c780d6d0e28"
    },
    "crossNavigation": {
      "inbounds": {
        "miningapp-manage": {
          "semanticObject": "miningapp",
          "action": "manage",
          "title": "{{miningapp-manage.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "app.miningjsonproject.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "app.miningjsonproject.view",
        "async": true,
        "viewPath": "app.miningjsonproject.view"
      },
      "routes": [
        {
          "name": "RouteCompanyView",
          "pattern": "",
          "target": [
            "TargetCompanyView"
          ]
        },
        {
          "name": "RouteDetailView",
          "pattern": "detail/{detailIndex}",
          "target": [
            "TargetDetailView"
          ]
        }
      ],
      "targets": {
        "TargetCompanyView": {
          "id": "CompanyView",
          "name": "CompanyView"
        },
        "TargetDetailView": {
          "id": "DetailView",
          "name": "DetailView"
        }
      }
    },
    "rootView": {
      "viewName": "app.miningjsonproject.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "appminingjsonproject"
  }
}
